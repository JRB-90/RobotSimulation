<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AvaloniaApp"
			 xmlns:material="clr-namespace:Material.Styles.Themes;assembly=Material.Styles"
             x:Class="AvaloniaApp.App">
    <Application.DataTemplates>
		<local:ViewLocator/>
    </Application.DataTemplates>

	<Application.Resources>
		<material:BundledTheme BaseTheme="Dark" PrimaryColor="Purple" SecondaryColor="Lime"/>
	</Application.Resources>

	<Application.Styles>
		<StyleInclude Source="avares://Material.Avalonia/Material.Avalonia.Templates.xaml" />
		<StyleInclude Source="avares://JSim.Avalonia/Styles/JSimStyles.axaml" />
		<StyleInclude Source="/Themes/DockMaterialDesignTheme.axaml" />
	</Application.Styles>
</Application>
