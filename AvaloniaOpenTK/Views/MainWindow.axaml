<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:AvaloniaOpenTK.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:controls="clr-namespace:AvaloniaOpenTK.CustomControls;assembly=AvaloniaOpenTK"
        mc:Ignorable="d" d:DesignWidth="600" d:DesignHeight="400"
        x:Class="AvaloniaOpenTK.Views.MainWindow"
		Width="600" Height="400"
		WindowStartupLocation="CenterScreen"
        Icon="/Assets/avalonia-logo.ico"
        Title="AvaloniaOpenTK">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

    <controls:OpenTKControl/>

</Window>
