<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:JSimControlGallery.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:controls="clr-namespace:JSimControlGallery.Controls;assembly=JSimControlGallery"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="400"
		Width="300" Height="400"
        x:Class="JSimControlGallery.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="JSimControlGallery">

    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>

    <!--<controls:MaterialControl
		Margin="10"
		Material="{Binding Material}"/>-->
	
	<ContentControl
		Margin="10"
		Content="{Binding MaterialControl}"/>

</Window>
